<!DOCTYPE html>
<html>
<head>
  <title>Animated Route</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
  <script defer src="https://use.fontawesome.com/releases/v5.0.6/js/all.js"></script>

  <link rel="stylesheet" href="/css/style.css">
  <meta name="viewport" content="width=device-width, initial-scale=1">

</head>

<body>
<div>
  <div id="locationForm">
    <div id="verticalDots">
      <i class="fas fa-ellipsis-v"></i>
    </div>
    <form id='mainForm'>
      <div class="form-group row origin-form input-block">
        <label for="fromLocation" class="col-sm-2 col-form-label"><i class="far fa-circle"></i></label>
        <div class="col-sm-10">
          <input type="text" class="form-control" id="origin-input" placeholder="From ...">
          <div id="clear-origin" class="input-block__form-control-clear">
            <i class="fas fa-times"></i>
          </div>
        </div>
      </div>
      <div class="form-group row input-block">
        <label for="toLocation" class="col-sm-2 col-form-label"><i class="far fa-flag"></i></label>
        <div class="col-sm-10">
          <input type="text" class="form-control" id="destination-input"  placeholder="To ...">
          <div id="clear-destination" class="input-block__form-control-clear">
            <i class="fas fa-times"></i>
          </div>
      </div>
      </div>
    </form>
  </div>
  <div id="map"></div>


  <!-- Replace the value of the key parameter with your own API key. -->
  <script type="text/javascript" src="AnimateRoute.js"></script>
  <script type="text/javascript" src="app.js"></script>
  <script type="text/javascript">
  </script>
  <script async defer
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBAAwbcguEMvnDbXchR-xKd6pH_l4YKetI&callback=initMap&libraries=places">
  </script>

</body>
</html>
